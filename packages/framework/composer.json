{
    "name": "buddywinangun/codeigniter-framework",
    "description": "CodeIgniter Xtend Framework",
    "type": "library",
    "homepage": "https://github.com/buddywinangun/codeigniter-xtend",
    "license": "MIT",
    "keywords": [
        "CodeIgniter",
        "Framework",
        "Xtend"
    ],
    "support": {
        "source": "https://github.com/buddywinangun/codeigniter-xtend"
    },
    "autoload": {
        "psr-4": {
            "CodeigniterXtend\\Framework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "buddywinangun",
            "email": "buddywinangun@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|~8.1.0",
        "twig/twig": "^3.8",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "codedungeon/phpunit-result-printer": "^0.32.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
